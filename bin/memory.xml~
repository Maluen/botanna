<memory>
    <!-- The bot name -->
    <name>Sbottonata</name>
    <!-- GDF -->
    <alias>
        <pattern>%botname% .*(avvia) .+gdf.*</pattern>
        <value>!gdf start</value>
    </alias>
    <alias>
        <pattern>%botname% .*(ferma|stoppa) .+gdf.*</pattern>
        <value>!gdf stop</value>
    </alias>
    <alias>
        <pattern>%botname% .*(entrare|partecipare|aggiungimi al) .+gdf.*</pattern>
        <value>!gdf join</value>
    </alias>
    <alias>
        <pattern>%botname% .*(toglimi|rimuovimi) .+gdf.*</pattern>
        <value>!gdf leave</value>
    </alias>
    <alias>
        <pattern>%botname% .*(recupera) .+gdf.*</pattern>
        <value>!gdf recover</value>
    </alias>
    <alias>
        <pattern>%botname% .*(skippa)</pattern>
        <value>!gdf skip</value>
    </alias>
    <!-- Conversation -->
</memory>
